package Methodoverriding;

public class overriding {
public static void main(String[] args) {
	A a = new B();
	a.m1(3);
}
}

class A{
void m1(int x)	{
	System.out.println("m1 from A");
	System.out.println(x);
}
}

class B extends A{
	void m1(int x) {
		System.out.println("m1 from B");
		System.out.println(x);
	}
	
}
