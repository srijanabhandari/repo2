package Constructor;
//Create a Car class with 4 attributes Color, Price, Wheels, Airbags. 
//Create a Constructor of 3 paramters and demonstrate the use of this keyword


public class Car {

	String Color;
	int price;
	int wheels;
	boolean airbags;
	
	Car(String Color , int price , int wheels) {
		this.Color = Color;
		this.price = price;
		this.wheels = wheels;
	}
}
