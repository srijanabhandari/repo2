package Constructor;

public class mainCar {

	public static void main(String[] args) {
		
		Car c = new Car("Blue" , 3400 , 4);
		System.out.println(c.Color);
		System.out.println(c.wheels);
		c.run();
		System.out.println("*************");
		System.out.println(Car.Color);
		System.out.println(Car.wheels);
		Car.run();

	}

}
