package herarchicalInheritance;

public  class School {
	String location;
	int price;
	
	School(String location, int price){
		this.location = location;
		this.price = price;
	}
  int learn(int a) {
	return price;
	
}
 }
 
class Teacher extends School {
  int salary;
  String subject;
	Teacher(String location, int price, int salary , String sub) {
		super(location, price);
		this.salary = salary;
		subject = sub;
	}
	
}

